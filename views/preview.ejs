                           <div data-v-52bfdbfa="" class="row icon-row">
                            <% Posts.forEach(function(post){ %>
                                <% if(post.selection == "Instagram"){ %>   
                                            <a data-v-52bfdbfa="" href="<%= post.linkgiven %>" target="_blank" class="btn-icon">
                                                <i data-v-52bfdbfa="" class="fab fa-instagram"></i>
                                            </a> 
                                  <% } else if( post.selection == "Twitter"){ %>  
                                        
                                            <a data-v-52bfdbfa="" href="<%= post.linkgiven %>" target="_blank" class="btn-icon">
                                                <i data-v-52bfdbfa="" class="fab fa-twitter"></i>
                                            </a> 
                                        
                                  <% } else if( post.selection == "YouTube"){ %>  
                                        
                                            <a data-v-52bfdbfa="" href="<%= post.linkgiven %>" target="_blank" class="btn-icon">
                                                <i data-v-52bfdbfa="" class="fab fa-youtube"></i>
                                            </a> 

                                 <% } else if( post.selection == "LinkedIn"){ %>  
                                    
                                        <a data-v-52bfdbfa="" href="<%= post.linkgiven %>" target="_blank" class="btn-icon">
                                            <i data-v-52bfdbfa="" class="fab fa-linkedinr"></i>
                                        </a> 
                                    
                                 <% } else { %>  
                                   
                                        <a data-v-52bfdbfa="" href="<%= post.linkgiven %>" target="_blank" class="btn-icon">
                                            <i data-v-52bfdbfa="" class="fab fa-github"></i>
                                        </a> 
                                 <% } %>                        
                 <% }) %>

               </div>
            </div>

            <div data-v-52bfdbfa="" class="link_list">
               <ul data-v-52bfdbfa="" style="padding-left: 0px;">
                  
                           <% Posts.forEach(function(post){ %>
                              <% if (post.title != null ){ %>
                                 <li data-v-52bfdbfa="" class="main-link">
                                 <a data-v-52bfdbfa="" href="<%= post.linkurl %>" target="_blank">
                                    <h6 data-v-52bfdbfa="" class="card-subtitle"><%= post.title  %></h6>
                                    <p data-v-52bfdbfa="" class="card-text"><%= post.description %></p>
                                 </a>
                                 </li>
                              <% } else{ %>
                                    

                              <% } %>
                     <% }) %>
                 
